  // 琛ㄦ儏鏀惧ぇ
  const owoBig = function () {
    let flag = 1, // 璁剧疆鑺傛祦闃€
      owo_time = "", // 璁剧疆璁℃椂鍣?      m = 3; // 璁剧疆鏀惧ぇ鍊嶆暟
    // 鍒涘缓鐩掑瓙
    let div = document.createElement("div");
    // 璁剧疆ID
    div.id = "owo-big";
    // 鎻掑叆鐩掑瓙
    let body = document.querySelector("body");
    body.appendChild(div);

    // 鐩戝惉 post-comment 鍏冪礌鐨勫瓙鍏冪礌娣诲姞浜嬩欢
    const observer = new MutationObserver(mutations => {
      mutations.forEach(mutation => {
        const addedNodes = mutation.addedNodes;
        // 鍒ゆ柇鏂板鐨勮妭鐐逛腑鏄惁鍖呭惈 OwO-body 绫诲悕鐨勫厓绱?        for (let i = 0; i < addedNodes.length; i++) {
          const node = addedNodes[i];
          if (
            node.nodeType === Node.ELEMENT_NODE &&
            node.classList.contains("OwO-body") &&
            !node.classList.contains("comment-barrage")
          ) {
            const owo_body = node;
            // 绂佺敤鍙抽敭锛堟墜鏈虹闀挎寜浼氬嚭鐜板彸閿彍鍗曪紝涓轰簡浣撻獙缁欑鐢ㄦ帀锛?            owo_body.addEventListener("contextmenu", e => e.preventDefault());
            // 榧犳爣绉诲叆
            owo_body.addEventListener("mouseover", handleMouseOver);
            // 榧犳爣绉诲嚭
            owo_body.addEventListener("mouseout", handleMouseOut);
          }
        }
      });
    });

    // 閰嶇疆 MutationObserver 閫夐」
    const config = { childList: true, subtree: true };

    // 寮€濮嬬洃鍚?    observer.observe(document.getElementById("post-comment"), config);

    function handleMouseOver(e) {
      if (e.target.tagName == "IMG" && flag) {
        flag = 0;
        // 绉诲叆100姣鍚庢樉绀虹洅瀛?        owo_time = setTimeout(() => {
          let height = e.target.clientHeight * m; // 鐩掑瓙楂?          let width = e.target.clientWidth * m; // 鐩掑瓙瀹?          let left = e.x - e.offsetX - (width - e.target.clientWidth) / 2; // 鐩掑瓙涓庡睆骞曞乏杈硅窛绂?          if (left + width > body.clientWidth) {
            left -= left + width - body.clientWidth + 10;
          } // 鍙宠竟缂樻娴嬶紝闃叉瓒呭嚭灞忓箷
          if (left < 0) left = 10; // 宸﹁竟缂樻娴嬶紝闃叉瓒呭嚭灞忓箷
          let top = e.y - e.offsetY; // 鐩掑瓙涓庡睆骞曢《閮ㄨ窛绂?
          // 璁剧疆鐩掑瓙鏍峰紡
          div.style.height = height + "px";
          div.style.width = width + "px";
          div.style.left = left + "px";
          div.style.top = top + "px";
          div.style.display = "flex";
          // 鍦ㄧ洅瀛愪腑鎻掑叆鍥剧墖
          div.innerHTML = `<img src="${e.target.src}">`;
        }, 100);
      }
    }

    function handleMouseOut(e) {
      // 闅愯棌鐩掑瓙
      div.style.display = "none";
      flag = 1;
      clearTimeout(owo_time);
    }
  };

